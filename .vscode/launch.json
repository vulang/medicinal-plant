{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--category", "jp_identification",
                "--limit", "500",
                "--format", "csv",
                "--output", "jp_identification.csv",
                "--include-details",
                "--download-photos",
                "--photo-dir", "photos"
            ]},
        {
            "name": "Python Debugger: JP Entry",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--start", "670",
                "--output", "jp_identification.csv",
                "--download-photos",
                "--photo-dir", "photos"
            ]}
    ]
}